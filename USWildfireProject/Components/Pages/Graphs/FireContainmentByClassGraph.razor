<RadzenText TextStyle="TextStyle.H5" Text="Average Fire Containment by Class (lower is better)" />

@if (IsDone)
{
    <RadzenChart>
        <RadzenColumnSeries Data="@averageContainmentByClass" CategoryProperty="Class" LineType="LineType.Solid" ValueProperty="AverageContainment" Title="Fire Class" />
        <RadzenCategoryAxis Step="1" />
    </RadzenChart>
}